{
  "name": "@ttab/api-client",
  "version": "0.6.0",
  "description": "Browser-friendly JavaScript client for TT Nyhetsbyrån public APIs.",
  "keywords": [
    "api",
    "client",
    "browser",
    "news",
    "content",
    "tt",
    "tt nyhetsbyrån"
  ],
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "standard",
    "bundle-test": "mkdir -p tmp && browserify -o tmp/test.bundle.js -d test/test-*",
    "test-browser-integration": "npm run bundle-test && osascript -e \"tell application \\\"Google Chrome\\\" to open location \\\"file://${PWD}/test/index.html#$TOKEN\\\"\"",
    "test-nodejs-integration": "mocha"
  },
  "browser": {
    "./index.js": "./browser.js"
  },
  "files": [
    "*.js"
  ],
  "author": "Fredrik Appelberg",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "browser-request": "^0.3.3",
    "debug": "^3.1.0",
    "eventemitter3": "^2.0.3",
    "request": "^2.83.0"
  },
  "peerDependencies": {}
}
